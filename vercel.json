
{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/blooks.json",
            "dest": "blooks.json"
        },
        {
            "src": "/(.+)",
            "dest": "static/$1"
        }
    ]
}
