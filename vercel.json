
{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/images/(.*)",
            "dest": "static/images/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "static/assets/$1"
        },
        {
            "src": "/(.+)",
            "dest": "index.js"
        }
    ]
}
